	<queryString>
		<![CDATA[select id_usuario,id_pedido,nombre,email,telefono,fecha, Sum(total_a_pagar) 
from usuariocliente join pedido  on  id_usuario= id_cliente
 join estado on pedido.id_estado=estado.id_estado 
 where estado='Pagado' and total_a_pagar>=100 order by total_a_pagar desc]]>
